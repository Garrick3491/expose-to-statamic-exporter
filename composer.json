{
    "name": "parallax/expose-statamic-export",
    "description": "A Pacakge to assist with exporting data from Expose Cake to Statamic Laravel",
    "type": "library",
    "require": {
        "laravel/laravel": "^9.0",
        "statamic/cms": "^3.4",
        "ueberdosis/tiptap-php": "^1.1",
        "netcarver/textile": "^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Parallax\\ExposeStatamicExport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parallax\\ExposeStatamicExport\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Simon Garrick",
            "email": "simon.garrick@parall.ax"
        }
    ],
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "laravel/pint": "^1.9"
    }
}
